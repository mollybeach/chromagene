    },
    "dependencies": {
        "axios": "^0.21.1",
        "build": "^0.1.4",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "heroku": "^7.56.1",
        "node": "^16.6.1",
        "path": "^0.12.7",
        "react": "^17.0.2",
        "react-app": "^1.1.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.2.0",
        "react-scripts": "^4.0.3",
        "sass": "^1.37.5",
        "serve": "^12.0.0",
